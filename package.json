{
  "name": "native_idle_time",
  "version": "1.0.1",
  "description": "Cross platform addon to get the user idle time.",
  "gypfile": true,
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/schlameel/native-idle-time.git"
  },
  "keywords": [
    "idle",
    "native",
    "windows",
    "mac",
    "linux"
  ],
  "author": "Schlameel",
  "bugs": "https://github.com/schlameel/native-idle-time/issues",
  "homepage": "https://github.com/schlameel/native-idle-time",
  "dependencies": {
    "nan": "^2.3.3",
    "node-pre-gyp": "^0.6.39"
  },
  "binary": {
    "module_name": "native_idle_time",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path": "{version}",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://github.com/schlameel/native-idle-time/releases/download/"
  },
  "scripts": {
    "preinstall": "npm install node-pre-gyp",
    "install": "node-pre-gyp install --fallback-to-build"
  }
}
